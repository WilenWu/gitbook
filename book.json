{
  "root": "./bookshelf/",
  "title": "书架",
  "author": "WilenWu",
  "language": "zh-hans",
  
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },  
  
"links" : {
    "sidebar" : {
        "<font size=6><i class='fa fa-home'></i></font>": "https://wilenwu.github.io"
    }
},
    
  "plugins": [
    "theme-comscore",
    "custom-favicon", 
    "insert-logo",
    "-sharing",  "-fontsettings",
    "search-pro",  "-search",  "-lunr",
    "expandable-chapters",
    "splitter",
    "anchor-navigation-ex",
    "hide-element",
    "tbfed-pagefooter",
    "github", 
    "mygitalk", 
    "prism",  "-highlight",  
    "copy-code-button",
    "codeblock-filename",
    "advanced-emoji",
    "flexible-alerts",
    "emphasize",
    "katex-plus",
    "mermaid",
    "pageview-count",
    "sitemap",
    "baidu"
  ],
  
  "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "favicon": "github32.png",
        "search-pro": {
          "cutWordLib": "nodejieba",
          "defineWord" : ["Gitbook Use"]
        },
    "insert-logo": {
        "url": "open_book.png",
        "style": "background: none; max-height: 30px; min-height: 30px"
      },   
       "anchor-navigation-ex": {
         "showLevel": true,
         "associatedWithSummary": false,
         "printLog": false,
         "multipleH1": true,
         "mode": "float"
        },
     "hide-element": {
          "elements": [".gitbook-link"]
        },
      "tbfed-pagefooter": {
        "copyright":"Copyright &copy WilenWu",
        "modify_label": "更新于 ",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
      },
       "github": {
         "url": "https://github.com/wilenwu/wilenwu.github.io"
      },
      "mygitalk": {
